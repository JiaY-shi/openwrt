
. /lib/functions/uci-defaults.sh

board_config_update

board=$(board_name)

case "$board" in
zn,m2)
	ucidef_set_led_netdev "power" "POWER" "blue:power" 
	ucidef_set_led_netdev "wan" "WAN" "blue:wan" "eth0"
	ucidef_set_led_netdev "lan3" "LAN3" "blue:lan3" "eth3"
	ucidef_set_led_netdev "lan2" "LAN2" "blue:lan2" "eth2"
	ucidef_set_led_netdev "lan1" "LAN1" "blue:lan1" "eth1"
	;;
*)
	;;
esac

board_config_flush

exit 0
